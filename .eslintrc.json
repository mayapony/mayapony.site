{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "prettier",
    "next/core-web-vitals"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error"
  },
  "ignorePatterns": [
    "*.config.cjs",
    "node_modules/*"
  ]
}
